<RulesDefinition>
  <rules>
    <rule>
      <name>t1</name>
      <left>Hello this is messageId 123456ID </left>
      <right>{{messageId}}</right>
      <operation>CONTAINS</operation>
    </rule>
    <rule>
      <name>t2</name>
      <left>TEST</left>
      <right>TEST</right>
      <operation>EQUAL</operation>
    </rule>
    <rule>
      <name>t3</name>
      <left>7</left>
      <right>10</right>
      <operation>NOT_GREATER</operation>
    </rule>
  </rules>
  <statements>
    <statement>
      <value>(t1|t2)&amp;(!(t3)|t2)</value>
      <actions>
        <action>
          <tag>action</tag>
          <value>BLOCK</value>
        </action>
        <action>
          <tag>code</tag>
          <value>33</value>
        </action>
      </actions>
    </statement>
  </statements>
